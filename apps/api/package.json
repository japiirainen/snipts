   },
   "scripts": {
      "build": "NODE_ENV=production tsc",
      "start": "TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling NODE_ENV=development nodemon --watch . --watch ../../packages --exec ts-node -r tsconfig-paths/register --transpile-only ./src/index.ts",
      "dev": "NODE_ENV=development tsc && node dist/index.js",
      "c": "tsc --noEmit",
      "test:coverage": "NODE_ENV=testing jest --coverage",
      "watch": "tsc-watch --onSuccess \"node --enable-source-maps .\" --preserveWatchOutput",
      "db": "docker-compose -f docker-compose-dev.yml up --build",
      "test": "NODE_ENV=testing jest --watchAll"
   }
